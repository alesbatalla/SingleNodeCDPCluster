{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_cw_name": {
            "defaultValue": "cdsw",
            "type": "String"
        },
        "disks_cw_OsDisk_1_33e850cbb9564fddb80a62948fffe504_externalid": {
            "defaultValue": "/subscriptions/8bfeca61-c921-4fa0-8ac0-eb62b7072a01/resourceGroups/SANS1WEURSGGENERIGENE001/providers/Microsoft.Compute/disks/cdsw_OsDisk_1",
            "type": "String"
        },
        "disks_cw_DataDisk_0_externalid": {
            "defaultValue": "/subscriptions/8bfeca61-c921-4fa0-8ac0-eb62b7072a01/resourceGroups/sans1weursggenerigene001/providers/Microsoft.Compute/disks/cdsw_DataDisk_0",
            "type": "String"
        },
        "disks_cw_DataDisk_1_externalid": {
            "defaultValue": "/subscriptions/8bfeca61-c921-4fa0-8ac0-eb62b7072a01/resourceGroups/sans1weursggenerigene001/providers/Microsoft.Compute/disks/cdsw_DataDisk_1",
            "type": "String"
        },
        "networkInterfaces_cw796_externalid": {
            "defaultValue": "/subscriptions/8bfeca61-c921-4fa0-8ac0-eb62b7072a01/resourceGroups/sans1weursggenerigene001/providers/Microsoft.Network/networkInterfaces/cdsw_ni_1",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2019-07-01",
            "name": "[parameters('virtualMachines_cw_name')]",
            "location": "westeurope",
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_D16_v3"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "OpenLogic",
                        "offer": "CentOS",
                        "sku": "7.7",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Linux",
                        "name": "[concat(parameters('virtualMachines_cw_name'), '_OsDisk_1')]",
                        "createOption": "FromImage",
                        "caching": "ReadWrite",
                        "managedDisk": {
                            "storageAccountType": "Standard_LRS"
                        },
                        "diskSizeGB": 30
                    },
                    "dataDisks": [
                        {
                            "lun": 0,
                            "name": "[concat(parameters('virtualMachines_cw_name'), '_DataDisk_0')]",
                            "createOption": "Attach",
                            "caching": "ReadOnly",
                            "writeAcceleratorEnabled": false,
                            "managedDisk": {
                                "storageAccountType": "StandardSSD_LRS",
                                "id": "[parameters('disks_cw_DataDisk_0_externalid')]"
                            },
                            "diskSizeGB": 256,
                            "toBeDetached": false
                        },
                        {
                            "lun": 1,
                            "name": "[concat(parameters('virtualMachines_cw_name'), '_DataDisk_1')]",
                            "createOption": "Attach",
                            "caching": "ReadOnly",
                            "writeAcceleratorEnabled": false,
                            "managedDisk": {
                                "storageAccountType": "StandardSSD_LRS",
                                "id": "[parameters('disks_cw_DataDisk_1_externalid')]"
                            },
                            "diskSizeGB": 256,
                            "toBeDetached": false
                        }
                    ]
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_cw_name')]",
                    "adminUsername": "cdsw",
                    "linuxConfiguration": {
                        "disablePasswordAuthentication": true,
                        "ssh": {
                            "publicKeys": [
                                {
                                    "path": "/home/cdsw/.ssh/authorized_keys",
                                    "keyData": "ssh-rsa  alejandro.batalla@extern.isban.com"
                                }
                            ]
                        },
                        "provisionVMAgent": true
                    },
                    "secrets": [],
                    "allowExtensionOperations": true,
                    "requireGuestProvisionSignal": true
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[parameters('networkInterfaces_cw796_externalid')]"
                        }
                    ]
                },
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true,
                        "storageUri": "https://sans1weursggenerigene001.blob.core.windows.net/"
                    }
                },
                "priority": "Spot",
                "evictionPolicy": "Deallocate",
                "billingProfile": {
                    "maxPrice": -1
                }
            }
        }
    ]
}